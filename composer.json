{
  "name": "alebedev80/doctrine_orm_cache_overflow",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mpdude/doctrine2"
    }
  ],
  "prefer-stable": true,
  "require": {
    "ext-redis": "*",
    "ext-pdo": "*",
    "doctrine/orm": "dev-fix-11112 as 2.17.1",
    "doctrine/cache": "2.2.0",
    "doctrine/dbal": "3.7.2",
    "symfony/cache": "v6.4.0"
  },
  "scripts": {
    "test:unit": "/usr/local/bin/docker exec -it lox24_wemomo-php-1 /srv/app/bin/phpunit --testsuite Unit"
  },

  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}